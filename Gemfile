source "https://rubygems.org"

# Core Jekyll
gem "jekyll", "~> 4.3"

# The Theme (Local Version - Faster)
gem "minimal-mistakes-jekyll"

# Plugins
group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
  gem "jekyll-include-cache"
end

# Linux Compatibility
gem "webrick"